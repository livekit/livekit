{
  "name": "livekit-ai-sdk-agent-example",
  "version": "1.0.0",
  "description": "Example TypeScript agent using AI SDK with LiveKit for voice interactions",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "livekit",
    "ai-sdk",
    "typescript",
    "voice-agent",
    "realtime-ai"
  ],
  "author": "LiveKit",
  "license": "Apache-2.0",
  "dependencies": {
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "express": "^4.18.2",
    "livekit-client": "^2.0.0",
    "livekit-server-sdk": "^2.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
